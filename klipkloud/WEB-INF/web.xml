<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app
   xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5">
   

  
  <display-name>Market Limit</display-name>
  <servlet>
    <servlet-name>marketlimit</servlet-name>
    <servlet-class>gsonutil.MarketLimitServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>marketlimit</servlet-name>
    <url-pattern>/marketlimit</url-pattern>
  </servlet-mapping>
  
	<welcome-file-list>
    <welcome-file>marketlimit</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.html</welcome-file>
	</welcome-file-list>
  

  
  
</web-app>
